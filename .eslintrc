{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
  },
  "rules": {
    "max-len": [0],
    "semi": [2, "never"],
    "func-names": [0],
    "camelcase": [0],
    "object-shorthand": [0],
    "quote-props": [0],
    "no-underscore-dangle": [0],
    "no-param-reassign": [0],
    "no-constant-condition": [0],
    "prefer-arrow-callback": [0],
    "comma-dangle": [2, "always-multiline"],
    "no-prototype-builtins": [0],
    "generator-star-spacing": [0],
    "no-await-in-loop": [0],
    "consistent-return": [0],
    "global-require": [1],
    "no-else-return": [0],
    "no-restricted-syntax": [0],
    "no-use-before-define": [0],
    "no-nested-ternary": [0],
    "arrow-body-style": [0],
    "no-bitwise": [0],
    "no-cond-assign": [0],
    "require-yield": [1],
    "import/no-named-as-default": [0],
    "import/prefer-default-export": [0],
    "import/no-extraneous-dependencies": [0],
    "import/extensions": [0],
    "import/no-unresolved": [0]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
